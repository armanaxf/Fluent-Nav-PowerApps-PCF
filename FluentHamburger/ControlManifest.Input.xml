<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="FluentHamburger" constructor="FluentHamburger" version="1.0.1" display-name-key="FluentHamburger" description-key="Fluent UI Hamburger Menu Button for Canvas Apps" control-type="virtual">
    <external-service-usage enabled="false">
    </external-service-usage>
    
    <!-- Input Properties -->
    <property name="Tooltip" display-name-key="Tooltip" description-key="Tooltip text for the hamburger button" of-type="SingleLine.Text" usage="input" required="false" default-value="Toggle navigation" />
    
    <!-- Common Events -->
    <common-event name="OnSelect" />
    
    <resources>
      <code path="index.ts" order="1"/>
      <platform-library name="React" version="16.14.0" />
      <platform-library name="Fluent" version="9.46.2" />
    </resources>
  </control>
</manifest>
