    <property name="Selected" display-name-key="Selected" description-key="Whether the card is currently selected" of-type="TwoOptions" usage="output"/>
    <property name="Clicked" display-name-key="Clicked" description-key="Triggers when the card is clicked (toggles true/false)" of-type="TwoOptions" usage="output"/>
    <resources>
      <code path="bundle.js" order="1"/>
      <platform-library name="React" version="16.14.0"/>
      <platform-library name="Fluent" version="9.46.2"/>
    </resources>
    <feature-usage/>
    <built-by name="pac" version="1.51.1"/>
  </control>
</manifest>